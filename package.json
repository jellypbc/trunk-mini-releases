{
  "name": "trunk-mini",
  "version": "0.1.4",
  "repository": "https://github.com/jellypbc/trunk-mini.git",
  "author": "Denny Luan <dennyluan@gmail.com>",
  "license": "MIT",
  "main": "index.js",
  "private": true,
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "dev": "next dev",
    "dev-worker": "miniflare --live-reload --debug",
    "build": "rm -rf dist && next build && npm run build-worker",
    "build-worker": "esbuild --bundle --format=esm --outfile=dist/index.mjs --tsconfig=worker/tsconfig.json worker/index.ts ",
    "format": "prettier --write './**/*.{js,jsx,json,ts,tsx,html,css,md}'",
    "check-types": "tsc -p tsconfig.json --noEmit && tsc -p worker/tsconfig.json --noEmit",
    "tauri": "tauri",
    "make": "rm -rf dist && next build && next export -o ./dist && npm run build-worker && yarn run tauri build --debug"
  },
  "dependencies": {
    "@cloudflare/workers-types": "^3.3.1",
    "@rocicorp/logger": "^2.2.0",
    "@rocicorp/reflect": "^0.6.0",
    "@rocicorp/reflect-server": "^0.11.0",
    "@supabase/supabase-js": "^1.30.7",
    "@tauri-apps/api": "^1.0.0-rc.5",
    "@types/node": "^17.0.14",
    "@types/react-dom": "^17.0.11",
    "bootstrap": "^4.6.0",
    "esbuild": "^0.14.18",
    "fuse.js": "^6.5.3",
    "miniflare": "^2.5.0",
    "nanoid": "^3.2.0",
    "next": "^12.1.0",
    "next-pwa": "^5.5.2",
    "prettier": "^2.5.1",
    "prosemirror-commands": "^1.2.1",
    "prosemirror-dropcursor": "^1.4.0",
    "prosemirror-example-setup": "^1.1.2",
    "prosemirror-gapcursor": "^1.2.1",
    "prosemirror-history": "^1.2.0",
    "prosemirror-keymap": "^1.1.5",
    "prosemirror-menu": "^1.1.4",
    "prosemirror-schema-basic": "^1.1.2",
    "prosemirror-schema-list": "^1.1.6",
    "prosemirror-state": "^1.3.4",
    "prosemirror-view": "^1.23.6",
    "react": "17.0.2",
    "react-bootstrap": "^1.5.2",
    "react-dom": "^17.0.2",
    "react-draggable": "^4.4.4",
    "react-hotkeys": "^1.1.4",
    "replicache-react": "^2.8.0",
    "sass": "^1.49.9",
    "styled-components": "^5.3.3",
    "typescript": "^4.6.2",
    "zod": "^3.11.6"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^1.0.0-rc.10",
    "@types/prosemirror-model": "^1.16.1",
    "@types/prosemirror-schema-basic": "^1.0.2",
    "@types/prosemirror-schema-list": "^1.0.3",
    "@types/prosemirror-state": "^1.2.8",
    "@types/react": "^17.0.39",
    "dotenv": "^16.0.0"
  }
}
